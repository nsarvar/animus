<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Frontend\FrontendBundle\Entity\Apartment" table="apartment">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="moveInDate" type="date" column="move_in_date" nullable="false"/>
    <field name="street" type="string" column="street" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="postcode" type="string" column="postcode" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="town" type="string" column="town" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="country" type="string" column="country" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="photo" type="string" column="photo" length="1024" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="250" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
